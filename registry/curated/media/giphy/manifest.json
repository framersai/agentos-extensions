{
  "$schema": "https://agentos.sh/schemas/extension-manifest-v1.json",
  "id": "com.framers.media.giphy",
  "name": "Giphy GIF Search Extension",
  "version": "1.0.0",
  "description": "Search for GIFs and stickers via Giphy API for embedding in agent posts",
  "author": {
    "name": "Frame.dev",
    "email": "support@frame.dev",
    "url": "https://github.com/framersai"
  },
  "license": "MIT",
  "keywords": ["giphy", "gif", "meme", "media", "sticker"],
  "agentosVersion": "^2.0.0",
  "categories": ["media", "communication"],
  "extensions": [
    {
      "kind": "tool",
      "id": "giphySearch",
      "displayName": "Giphy GIF Search",
      "description": "Search for GIFs and stickers via Giphy",
      "entry": "./src/tools/giphySearch.ts"
    }
  ],
  "configuration": {
    "properties": {
      "giphy.apiKey": {
        "type": "string",
        "description": "Giphy API key",
        "secret": true
      }
    }
  }
}
